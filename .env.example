# Required API Keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
QDRANT_API_KEY=your_qdrant_api_key

# Database Configuration
QDRANT_URL=your_qdrant_url

# Application Configuration
NODE_ENV=production  # Options: development, production, test
NEXT_PUBLIC_SITE_URL=https://your-production-domain.com

# Security Configuration
SESSION_SECRET=your_long_random_string  # Min 32 characters
ENCRYPTION_KEY=your_32_byte_encryption_key  # For sensitive data encryption
CORS_ALLOWED_ORIGINS=https://your-production-domain.com,https://api.your-production-domain.com

# Rate Limiting (Production Values)
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes in milliseconds
RATE_LIMIT_MAX_REQUESTS=100  # Requests per window per IP

# Cache Configuration
REDIS_URL=your_redis_url  # Optional: For session/cache management
CACHE_TTL=3600  # Cache time-to-live in seconds

# Logging Configuration
LOG_LEVEL=info  # Options: debug, info, warn, error
LOG_FORMAT=json  # Options: json, pretty

# Optional: High Availability Configuration
FALLBACK_API_URL=your_fallback_api_url  # Backup API endpoint
MAX_RETRIES=3  # Number of retries for failed operations

# Note: Replace all placeholder values with actual secure values
# Never commit actual .env files to version control
# Keep your production values secure and separate
